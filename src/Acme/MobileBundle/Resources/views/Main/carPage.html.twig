
{% extends 'AcmeMobileBundle:Main:baseLayer.html.twig' %}
    {% block content %}
    
<div class="moduletable_Breadcrumbs">
    <div class="breadcrumbs_Breadcrumbs">
        <a href="/free_2013_virtuemart_2_0_22_a/" class="pathway">Home</a> &gt; <a href="/free_2013_virtuemart_2_0_22_a/index.php/online-store" class="pathway">Online Store</a> &gt; <a href="/free_2013_virtuemart_2_0_22_a/index.php/online-store/minivan" class="pathway">Minivan</a> &gt; Ipsum dolor sit...
    </div>
</div>
<div class="error err-space">
    <div id="system-message-container"></div>
</div>
<div class="content-indent">
    <div class="productdetails-view">
        <div class="icons">
            <a class="modal" rel="{handler: 'iframe'<br> size: {x: 700<br> y: 550}}" title="Email" href="/free_2013_virtuemart_2_0_22_a/index.php/online-store/minivan/product-18-detail/recommend?tmpl=component"><img src="{{ asset('images/emailButton.png') }}" alt="Email"  /></a>
            <div class="clear"></div>
        </div>
        <div class="wrapper2">
            <div class="fright">
                <h1 class="title">{{ car.model }}</h1>
                <div class="s_desc">
                    <table width="100%" cellspacing="0" cellpadding="5">
                        <tr>
                            <td width="50%" valign="top">Тип кузова автомобиля: {{ car.categoryid.category }}</td><td valign="top">Вид топлива: {{ car.fuelid.fuel }}</td>
                         </tr>
                            <td width="50%" valign="top">Состояние: {{ car.conditionid.condition }}</td><td valign="top">Пробег: {{ car.mileage }}</td>
                        <tr>
                            <td width="50%" valign="top">Первая регистрация: {{ car.firstregistration }}</td><td valign="top">Мощность двигателя: {{ car.power }}</td>
                         </tr>
                        <tr>
                            <td width="50%" valign="top">Тип коробки передач: {{ car.gearboxid.gearbox }}</td><td valign="top"></td>
                         </tr>
                    </table>
                </div>
                <div class="product-box2">
                    <div class="rating">
                        <span class="vote">
                            <span title=" Rating: /5" class="vmicon ratingbox" style="display:inline-block;">
                                <span class="stars-orange" style="width:%"></span>
                            </span>
                            <span class="rating-title">Доставка и оплата</span>
						</span>
                    </div>
                    <div class="spacer-buy-area">
                        <div class="addtocart-area">
                            <div class="addtocart-area">
                                <form method="post" class="product js-recalculate" action="/free_2013_virtuemart_2_0_22_a/index.php/online-store">
                                    <input name="quantity" type="hidden" value="1" />
                                    <div class="addtocart-bar2">
                                        <script src="{{ asset('js/mylib8.js') }}" type="text/javascript"></script>
                                        <div class="wrapper">
                                            <div class="price">
                                                <div class="product-price" id="productPrice18">
                                                    <strong>Price: </strong>
                                                    <span class="price-crossed" >
                                                        <div style="display : block;" >Строк доставки: <span>20 дней</span></div>
                                                    </span>
                                                    <div class="PricesalesPrice" style="display : block;" >Цена автомобиля: <span class="PricesalesPrice" >{{ (car.price * app.session.get('rate'))|number_format(0, '.', ' ') }} {{ app.session.get('symb') }}</span></div>
                                                    <div class="PricepriceWithoutTax" style="display : block;" >Предоплата: <span class="PricepriceWithoutTax" >{{ (car.price * app.session.get('rate') * 0.05)|number_format(0, '.', ' ') }} {{ app.session.get('symb') }}</span></div>
                                                    <div class="PricediscountAmount" style="display : block;" >Растаможка: <span class="PricediscountAmount" >{{ (car.price * app.session.get('rate') * 0.10)|number_format(0, '.', ' ') }} {{ app.session.get('symb') }}</span></div>
                                                    <div class="PricetaxAmount" style="display : block;" >Доставка: <span class="PricetaxAmount" >{{ (car.price * app.session.get('rate') * 0.25)|number_format(0, '.', ' ') }} {{ app.session.get('symb') }}</span></div>
                                                </div>
                                            </div>
                                        </div>
                                        <br/>
                                        <div class="wrapper">
                                            <div class="fright">
                                                <div class="ask-a-question">
                                                    <a class="ask-a-question" href="/free_2013_virtuemart_2_0_22_a/" >Ask a question about this product</a>
                                                </div>
                                                <div class="manufacturer">
                                                    <span class="bold">Manufacturer: </span><a class="modal" rel="{handler: 'iframe'<br> size: {x: 350<br> y: 300}}" href="/free_2013_virtuemart_2_0_22_a/index.php/online-store/manufacturer-default-layout/manufacturers/brand-example-5?tmpl=component">Manufacturer-5</a>
                                                </div>
                                            </div>
                                            <span class="addtocart-button">
                                                <i>&gt;</i>
                                                <input type="submit" name="addtocart"  class="addtocart-button cart-click2" value="Спросить" title="Add to Cart" />
                                            </span>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <input type="hidden" class="pname" value="Ipsum dolor sit amet conse ctetur"/>
                                    <input type="hidden" name="option" value="com_virtuemart"/>
                                    <input type="hidden" name="view" value="cart"/>
                                    <noscript><input type="hidden" name="task" value="add"/></noscript>
                                    <input type="hidden" name="virtuemart_product_id[]" value="18"/>
                                </form>
                                <div class="clear"></div>
                            </div>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="fleft">
                <div id="products_example">
                    <div id="products" class="">
                        <div class="slides_container">
                            {% set photos = car.photo|split(';') %}
                            <div class="slide"><a title="thumb_01/product18_thumb01" class="jqzoom modal" href="{{ photos[0] }}"><img src="{{ photos[0] }}" width="220px" height="165px" alt="{{ car.model }}" class="productimage" /></a><span class="vm-img-desc">{{ car.model }}</span></div>
                            {% for photo in 1..photos|length-1 %}
                              <div class="slide"><a title="thumb_01/product18_thumb01" class="jqzoom modal" href="{{ photos[photo] }}"><img src="{{ photos[photo] }}" width="220px" height="165px"  alt="thumb_01/product18_thumb01" class="productimage" /></a></div>
                            {% endfor %}  
                        </div>
                        <ul id="carousel" class="pagination jcarousel-skin-tango">
                            <li><a title="product18" 1 href="{{ photos[0] }}"><img src="{{ photos[0] }}" width="220px" height="165px" alt="product18" class="productimage" /></a></li>
                            {% for photo in 1..photos|length-1 %}
                              <li><a title="thumb_01/product18_thumb01" 1 href="{{ photos[photo] }}"><img src="{{ photos[photo] }}" width="220px" height="165px" alt="thumb_01/product18_thumb01" class="productimage" /></a></li>
                            {% endfor %}  
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="clear"></div>
        <div class="responsive-tabs">
            <h2>Характеристики</h2>
            <div>
                <div class="desc"> 
                    <div class="text"><p>{{ car.tecdata|raw }}</p> </div>
                </div>
			</div>
            <h2>Описание</h2>
            <div>
                <div class="customer-reviews">
                    <form method="post" action="/free_2013_virtuemart_2_0_22_a/index.php/online-store/minivan/product-18-detail" name="reviewForm" id="reviewform">
                        <div class="list-reviews">
                            <span class="step">{{ car.description|raw }}</span>
                            <div class="clear"></div>
                        </div>
                        <input type="hidden" name="virtuemart_product_id" value="18"/>
                        <input type="hidden" name="option" value="com_virtuemart"/>
                        <input type="hidden" name="virtuemart_category_id" value="4"/>
                        <input type="hidden" name="virtuemart_rating_review_id" value="0"/>
                        <input type="hidden" name="task" value="review"/>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="{{ asset('js/slides.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('js/mylib9.js') }}" type="text/javascript"></script>
</div>
    {% endblock %}